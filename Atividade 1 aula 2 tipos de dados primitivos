#include <stdio.h>

int main() {
    // Declaração e inicialização das variáveis
    int idade = 25;
    float altura = 1.75f;
    char letra = 'A';
    double salario = 4500.50;

    // Impressão dos valores das variáveis
    printf("Idade: %d\n", idade);
    printf("Altura: %.2f\n", altura);
    printf("Letra: %c\n", letra);
    printf("Salário: %.2f\n", salario);

    return 0;
}
